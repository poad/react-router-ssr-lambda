{
  "name": "@remix-ssr-lambda/app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "eslint:check": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
